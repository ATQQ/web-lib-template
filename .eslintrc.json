{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "import/extensions":"off",
        "import/no-unresolved":"off",
        "import/no-extraneous-dependencies":"off",
        "no-shadow":"off",
        "no-unused-vars":"off"
    }
}
